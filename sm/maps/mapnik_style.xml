<?xml version="1.0" encoding="utf-8"?>
 <Map background-color="#ffffff">
	<FontSet name="book-fonts">
		<Font face-name="DejaVu Sans Book" />
	</FontSet>
	<Style name="layermain">
        <Rule>
			<LineSymbolizer stroke="#0000FF" stroke-width="1" stroke-linejoin="round" stroke-linecap="round" />
			<TextSymbolizer name="[name]" fontset-name="book-fonts" size="14" fill="#000000" halo-radius="1" placement="line" />
		</Rule>
        <Rule>
            <Filter>[amenity] = 'restaurant' </Filter>
			<LineSymbolizer stroke="#00FF00" stroke-width="4" stroke-linejoin="round" stroke-linecap="round" />
		</Rule>
        <Rule>
            <Filter>[aeroway] = 'runway' </Filter>
			<LineSymbolizer stroke="#000000" stroke-width="6" stroke-linejoin="round" stroke-linecap="round" />
			<TextSymbolizer name="[ref]" fontset-name="book-fonts" size="20" fill="#000000" halo-radius="1" placement="line" />
		</Rule>
        <Rule>
            <Filter>[aeroway] = 'taxiway' </Filter>
			<LineSymbolizer stroke="#000000" stroke-width="2" stroke-linejoin="round" stroke-linecap="round" />
			<TextSymbolizer name="[ref]" fontset-name="book-fonts" size="20" fill="#000000" halo-radius="1" placement="line" />
		</Rule>
	</Style>
	<Layer name="layermain" status="on">
		<StyleName>layermain</StyleName>
		<Datasource>
			<Parameter name="type">osm</Parameter>
			<Parameter name="file">map.osm</Parameter>
		</Datasource>
	</Layer>
 </Map>

